<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
		<referenceContainer name="page.main.title">
            <block class="Magento\Sales\Block\Order\Info" name="order.status" template="order/order_status.phtml"/>
		</referenceContainer>	
        <referenceContainer name="content">
            <block class="Vuleticd\OrderHistory\Block\Order\Old" name="order.old" template="order/old.phtml" before="sales.order.info.links"/>  
        </referenceContainer>
    </body>
</page>
